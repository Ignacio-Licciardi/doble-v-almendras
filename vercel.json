{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/**/*.{js,ts,jsx,tsx}": {
      "runtime": "@vercel/node"
    }
  },
  "images": {
    "domains": [],
    "formats": ["image/webp", "image/avif"]
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}